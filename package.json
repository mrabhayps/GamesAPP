    "dotenv": "^8.2.0",
    "elastic-apm-node": "^3.7.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.13.0",
    "fs": "0.0.1-security",
    "jose": "^4.14.4",
    "jsonwebtoken": "^8.5.1",
    "kafka-node": "^5.0.0",
    "lodash.isarray": "3.0.4",
    "lodash.isfunction": "3.0.6",
    "mongoose": "^5.12.14",
    "moniker": "^0.1.2",
    "multer": "^1.4.2",
    "mysql2": "^2.0.1",
