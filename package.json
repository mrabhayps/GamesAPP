{
  "name": "gamesapp",
  "version": "1.0.0",
  "description": "GamesAPP Single Plateform Multiple game",
  "main": "index.js",
  "scripts": {
    "start": "ts-node build.ts && tsc && cp -r server/api/config dist/api/config && cp server/common/indusinduat-publickey.txt dist/common/indusinduat-publickey.txt && pm2 start dist/index.js --time",
    "restart": "ts-node build.ts && tsc && cp -r server/api/config dist/api/config && cp server/common/indusinduat-publickey.txt dist/common/indusinduat-publickey.txt && pm2 reload dist/index.js --time",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "lint": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet",
    "lint:fix": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet --fix",
    "test": "mocha test/**/*.ts --exit",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit",
    "StartKafkaConsumer": "pm2 start dist/kafka/consumer.js --time",
    "RestartKafkaConsumer": "pm2 reload dist/kafka/consumer.js --time"
  },
  "dependencies": {
    "agora-access-token": "^2.0.4",
